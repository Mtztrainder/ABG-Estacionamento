      <Button fx:id="btConfirmar" layoutX="30.0" layoutY="276.0" mnemonicParsing="false" onAction="#onActionConfirmar" prefHeight="43.0" prefWidth="149.0" text="Confirmar" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <ComboBox fx:id="cbMarca" layoutX="30.0" layoutY="175.0" prefHeight="30.0" prefWidth="426.0" promptText="" style="-fx-font-size: 14;" />
   </children>
</AnchorPane>
