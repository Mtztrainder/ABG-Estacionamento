<AnchorPane onKeyPressed="#onKeyPressed" prefHeight="350.0" prefWidth="477.0" stylesheets="@estilo.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unoeste.fipp.andrebrunogabriel.abgestacionamentosfipp.abgestacionamento.CadModelo">
   <children>
      <Label layoutX="32.0" layoutY="38.0" prefHeight="17.0" prefWidth="67.0" text="Código" />
      <TextField fx:id="tfCodigo" alignment="CENTER_RIGHT" editable="false" layoutX="32.0" layoutY="55.0" prefHeight="25.0" prefWidth="67.0">
         <font>
            <Font size="14.0" />
         </font></TextField>
      <Label layoutX="32.0" layoutY="98.0" prefHeight="17.0" prefWidth="426.0" text="Descrição" />
      <TextField fx:id="tfDesc" layoutX="32.0" layoutY="115.0" prefHeight="30.0" prefWidth="426.0" promptText="Digite a descrição da marca...">
